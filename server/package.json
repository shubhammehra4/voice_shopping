{
  "name": "shopping-app",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts test/**/*.test.ts",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "postinstall": "npm run build:ts",
    "start": "fastify start -l info dist/index.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev:check": "ts-node src/index.ts",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l debug -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^4.0.0",
    "@fastify/cors": "^7.0.0",
    "@fastify/sensible": "^4.0.0",
    "@fastify/swagger": "^6.0.1",
    "@prisma/client": "^3.14.0",
    "close-with-grace": "^1.1.0",
    "dotenv": "^16.0.1",
    "fastify": "^3.0.0",
    "fastify-cli": "^3.1.0",
    "fastify-plugin": "^3.0.0",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "prisma": "^3.14.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "prisma-dbml-generator": "^0.9.0",
    "tap": "^16.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.4"
  }
}
